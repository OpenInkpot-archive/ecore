         libxcb-test0-dev, libxcb-xinerama0-dev, libxcb-xfixes0-dev,
         libxcb-sync0-dev, libxcb-shape0-dev, libxcb-screensaver0-dev,
         libxcb-render0-dev, libxcb-randr0-dev, libxcb-dpms0-dev,
